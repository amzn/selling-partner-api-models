  "properties": {
    "reportSpecification": {
      "type": "object",
      "description": "Report input summary which includes the marketplace and selling program.",
      "examples": [
        {
          "reportType": "GET_VENDOR_FORECASTING_REPORT",
          "reportOptions": {
            "sellingProgram": "RETAIL"
          },
          "marketplaceIds": [
